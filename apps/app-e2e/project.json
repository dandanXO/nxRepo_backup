{
  "name": "app-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/app-e2e/cypress.config.ts",
        "testingType": "e2e"
      },
      "configurations": {
        "dev-in": {
          "devServerTarget": "app:serve-in"
        },
        "dev-pk": {
          "devServerTarget": "app:serve-pk"
        },
        "dev-bd": {
          "devServerTarget": "app:serve-bd"
        },
        "production": {
          "devServerTarget": "app:serve:production-in"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/app-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["app"]
}
