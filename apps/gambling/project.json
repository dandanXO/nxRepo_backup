{
  "name": "gambling",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/gambling/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/gambling",
        "index": "apps/gambling/src/index.html",
        "generateIndexHtml": false,
        "baseHref": "/",
        "main": "apps/gambling/src/main.tsx",
        "tsConfig": "apps/gambling/tsconfig.app.json",
        "assets": ["apps/gambling/src/favicon.ico", "apps/gambling/src/assets"],
        "styles": [],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/gambling/build/webpack.config.main.js",
        "postcssConfig": "apps/gambling/build/postcss.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "dev-brazil": {
          "fileReplacements": [
            {
              "replace": "apps/gambling/src/environments/environment.ts",
              "with": "apps/gambling/src/environments/environment.dev.brazil.ts"
            },
            {
              "replace": "apps/gambling/src/index.html",
              "with": "apps/gambling/src/environments/index.brazil.html"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "prod-brazil": {
          "fileReplacements": [
            {
              "replace": "apps/gambling/src/environments/environment.ts",
              "with": "apps/gambling/src/environments/environment.prod.brazil.ts"
            },
            {
              "replace": "apps/gambling/src/index.html",
              "with": "apps/gambling/src/environments/index.brazil.html"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "dev-bmw": {
          "fileReplacements": [
            {
              "replace": "apps/gambling/src/environments/environment.ts",
              "with": "apps/gambling/src/environments/environment.dev.bmw.ts"
            },
            {
              "replace": "apps/gambling/src/index.html",
              "with": "apps/gambling/src/environments/index.bmw.html"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "dev-legend": {
          "fileReplacements": [
            {
              "replace": "apps/gambling/src/environments/environment.ts",
              "with": "apps/gambling/src/environments/environment.dev.legend.ts"
            },
            {
              "replace": "apps/gambling/src/index.html",
              "with": "apps/gambling/src/environments/index.bmw.html"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "gambling:build:development"
        },
        "production": {
          "buildTarget": "gambling:build:production",
          "hmr": false
        },
        "dev-bmw": {
          "buildTarget": "gambling:build:dev-bmw"
        },
        "dev-legend": {
          "buildTarget": "gambling:build:dev-legend"
        },
        "dev-brazil":  {
          "buildTarget": "gambling:build:dev-brazil"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/gambling/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "gambling:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/gambling/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
