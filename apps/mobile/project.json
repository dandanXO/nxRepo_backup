{
    "name": "mobile",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/mobile/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/webpack:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "babel",
                "outputPath": "dist/apps/mobile",
                "baseHref": "/",
                "index": "apps/mobile/src/index.html",
                "main": "apps/mobile/src/main.tsx",
                "polyfills": "apps/mobile/src/polyfills.ts",
                "tsConfig": "apps/mobile/tsconfig.app.json",
                "assets": [
                    "apps/mobile/src/favicon.ico",
                    "apps/mobile/src/assets"
                ],
                "styles": [],
                "scripts": [],
                "webpackConfig": "apps/mobile/webpack.config.js",
                "isolatedConfig": true
            },
            "configurations": {
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true,
                },
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.prod.ts"
                        }
                    ]
                },
                "development-in": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true,
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.in.dev.ts"
                        }
                    ]
                },
                "production-in": {
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.in.prod.ts"
                        }
                    ]
                },
                "development-pk": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true,
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.pk.dev.ts"
                        }
                    ]
                },
                "production-pk": {
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.pk.prod.ts"
                        }
                    ]
                },
                "development-bd": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true,
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.bd.dev.ts"
                        }
                    ]
                },
                "production-bd": {
                    "fileReplacements": [
                        {
                            "replace": "apps/mobile/src/environments/environment.ts",
                            "with": "apps/mobile/src/environments/environment.bd.prod.ts"
                        }
                    ]
                }
            }
        },
        "serve-build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "compiler": "babel",
            "outputPath": "dist/apps/mobile",
            "baseHref": "/",
            "main": "apps/mobile/src/main.tsx",
            "polyfills": "apps/mobile/src/polyfills.ts",
            "tsConfig": "apps/mobile/tsconfig.app.json",
            "assets": [
              "apps/mobile/src/favicon.ico",
              "apps/mobile/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/mobile/webpack.config.js",
            "isolatedConfig": true
          },
          "configurations": {
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.prod.ts"
                }
              ]
            },
            "development-in": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true,
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.in.dev.ts"
                }
              ]
            },
            "production-in": {
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.in.prod.ts"
                }
              ]
            },
            "development-pk": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true,
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.pk.dev.ts"
                }
              ]
            },
            "production-pk": {
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.pk.prod.ts"
                }
              ]
            },
            "development-bd": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true,
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.bd.dev.ts"
                }
              ]
            },
            "production-bd": {
              "fileReplacements": [
                {
                  "replace": "apps/mobile/src/environments/environment.ts",
                  "with": "apps/mobile/src/environments/environment.bd.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
            "executor": "@nrwl/webpack:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "mobile:build",
                "hmr": true
            },
            "configurations": {
                "development": {
                    "buildTarget": "mobile:build:development"
                },
                "production": {
                    "buildTarget": "mobile:build:production",
                    "hmr": false
                },
                "development-in": {
                    "buildTarget": "mobile:build:development-in"
                },
                "production-in": {
                    "buildTarget": "mobile:build:production-in",
                    "hmr": false
                },
                "development-pk": {
                    "buildTarget": "mobile:build:development-pk"
                },
                "production-pk": {
                    "buildTarget": "mobile:build:production-pk",
                    "hmr": false
                },
                "development-bd": {
                    "buildTarget": "mobile:build:development-bd"
                },
                "production-bd": {
                    "buildTarget": "mobile:build:production-bd",
                    "hmr": false
                }
            }
        },
        "serve:in": {
            "executor": "@nrwl/web:dev-server",
            "defaultConfiguration": "development",
            "options": {

            },
            "configurations": {
                "development": {
                  "buildTarget": "mobile:serve-build:development-in",
                  "hmr": true
                },
                "production": {
                  "buildTarget": "mobile:serve-build:production-in",
                  "hmr": false
                }
            }
        },
        "serve:pk": {
            "executor": "@nrwl/web:dev-server",
            "defaultConfiguration": "development",
            "options": {

            },
            "configurations": {
                "development": {
                    "buildTarget": "mobile:serve-build:development-pk",
                    "hmr": true
                },
                "production": {
                  "buildTarget": "mobile:serve-build:production-pk",
                  "hmr": false
                }
            }
        },
        "serve:bd": {
            "executor": "@nrwl/web:dev-server",
            "defaultConfiguration": "development",
            "options": {

            },
            "configurations": {
                "development": {
                    "buildTarget": "mobile:serve-build:development-bd",
                    "hmr": true
                },
                "production": {
                    "buildTarget": "mobile:serve-build:production-bd",
                    "hmr": false
                }
            }
        },
        "serve2-in": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "defaultConfiguration": "development",
                "buildTarget": "mobile:build:development-in"
            },
            "configurations": {
                "development": {
                    "hmr": true
                }
            }
        },
        "serve2-pk": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "defaultConfiguration": "development",
                "buildTarget": "mobile:build:development-pk"
            },
            "configurations": {
                "development": {
                    "hmr": true
                }
            }
        },
        "serve2-bd": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "defaultConfiguration": "development",
                "buildTarget": "mobile:build:development-bd"
            },
            "configurations": {
                "development": {
                    "hmr": true
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/mobile/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/apps/mobile"],
            "options": {
                "jestConfig": "apps/mobile/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "storybook": {
            "executor": "@nrwl/storybook:storybook",
            "options": {
                "uiFramework": "@storybook/react",
                "port": 9000,
                "configDir": "apps/mobile/.storybook"
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@nrwl/storybook:build",
            "outputs": ["{options.outputDir}"],
            "options": {
                "uiFramework": "@storybook/react",
                "configDir": "apps/mobile/.storybook",
                "outputDir": "dist/storybook/mobile"
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    },
    "tags": []
}
