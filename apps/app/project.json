{
  "name": "app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/app",
        "baseHref": "/",
        "main": "apps/app/src/main.in.tsx",
        "tsConfig": "apps/app/tsconfig.app.json",
        "assets": ["apps/app/src/app/ui/components/images"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/app/build/webpack.config.main.js",
        "postcssConfig": "apps/app/build/postcss.config.js",
        "isolatedConfig": true
      },
      "configurations": {
        "production-in": {
          "main": "apps/app/src/main/main.in.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/india/environment.in.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "production-pk": {
          "main": "apps/app/src/main/main.pk.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/pakistan/environment.pk.prod.ts"
            }
          ],
          "webpackConfig": "apps/app/build/webpack.config.main.pk.js",
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "production-bd": {
          "main": "apps/app/src/main/main.bd.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/bangladesh/environment.bd.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "production-mx": {
          "main": "apps/app/src/main/main.mx.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/mexico/environment.mx.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "production-ph": {
          "main": "apps/app/src/main/main.ph.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/philippines/environment.ph.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve-dev": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/app",
        "baseHref": "/",
        "index": "apps/app/src/index.html",
        "generateIndexHtml": false,
        "main": "apps/app/src/main.in.tsx",
        "tsConfig": "apps/app/tsconfig.app.json",
        "assets": ["apps/app/src/app/ui/components/images"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/app/build/webpack.config.main.js",
        "postcssConfig": "apps/app/build/postcss.config.js",
        "isolatedConfig": true
      },
      "configurations": {
        "development-in": {
          "main": "apps/app/src/main/main.in.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/india/environment.in.dev.ts"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "development-pk": {
          "main": "apps/app/src/main/main.pk.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/pakistan/environment.pk.dev.ts"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "development-bd": {
          "main": "apps/app/src/main/main.bd.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/bangladesh/environment.bd.dev.ts"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "development-mx": {
          "main": "apps/app/src/main/main.mx.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/mexico/environment.mx.dev.ts"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "development-ph": {
          "main": "apps/app/src/main/main.ph.tsx",
          "fileReplacements": [
            {
              "replace": "apps/app/src/environments/environmentModule/environment.ts",
              "with": "apps/app/src/environments/environmentModule/philippines/environment.ph.dev.ts"
            }
          ],
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "serve-in": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app:serve-dev:development-in"
        }
      },
      "defaultConfiguration": "development"
    },
    "serve-pk": {
      "executor": "@nx/web:dev-server",
      "options": {
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app:serve-dev:development-pk"
        }
      },
      "defaultConfiguration": "development"
    },
    "serve-bd": {
      "executor": "@nx/web:dev-server",
      "options": {
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app:serve-dev:development-bd"
        }
      },
      "defaultConfiguration": "development"
    },
    "serve-mx": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app:serve-dev:development-mx"
        }
      },
      "defaultConfiguration": "development"
    },
    "serve-ph": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "app:serve-dev:development-ph"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/app/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/app"],
      "options": {
        "jestConfig": "apps/app/build/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": []
}
