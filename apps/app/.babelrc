{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "useBuiltIns": "usage",
        "corejs": {
          "version": "3.8",
          "proposals": true
        },
        "targets": {
          "android": "53",
          "chrome": "53"
        },
        "modules": false
      }
    ],
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-destructuring"],
    ["@babel/plugin-transform-async-to-generator"],
    ["styled-components", { "pure": true, "ssr": true }]
  ]
}
