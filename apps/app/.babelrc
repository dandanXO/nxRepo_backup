{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "useBuiltIns": "usage",
        "corejs": {
          "version": "3.8",
          "proposals": false
        },
        "targets": [
          "last 2 version",
          "chrome >= 58",
          "android >= 58",
          "not IE 9-11"
        ]
      }
    ],
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["styled-components", { "pure": true, "ssr": true }]
  ]
}
