{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "bugfixes": true,
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": "cover 100%",
        "modules": false,
        "forceAllTransforms": true,
        "spec": true
      }
    ],
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ],
    ["styled-components", { "pure": true, "ssr": true }]
  ]
}
